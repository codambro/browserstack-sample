{
  "compilerOptions": {
    "module": "commonjs",
    "downlevelIteration": true,
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "*.ts",
  ]
}
